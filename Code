# Answer to Exercise 5
import math

q = (3*5)/(2+3)
print('q = ',q)

w = math.sqrt(7+9*2)
print('w = ',w)

e = (4-7)**3
print('e = ',e)

r = math.pow(-19+100,1/4)
print('r = ',r)

t = 6 % 4
print('t = ',t)
