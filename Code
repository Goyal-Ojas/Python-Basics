print(a + b)

# Answer to Exercise 2
def tic_tac_toe(board):
    for row in board:
        print("|".join(row))
        print("-" * 9)

empty_board = [
    [" ", " ", " "],
    [" ", " ", " "],
    [" ", " ", " "]
]

tic_tac_toe(empty_board)

# Answer to Exercise 3
str1 = "Arizona"
str2 = " State"
str3 = " University"

result_string = str1 + str2 + str3

print(result_string)

# Answer to Exercise 4
